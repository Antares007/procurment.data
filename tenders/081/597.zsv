	$('#showtdocs').button({icons:{primary: "ui-icon-triangle-1-s"}}).click(function(){ $.get('engine/controller.php', {action:'app_tdocs', app_id:'81597'}, function(data){$('#app_tdocs').html(data);});})

	$('#pay_select_bank').change(function(){
		
